{
  "compilerOptions": {
    "module": "CommonJS",
    "moduleResolution": "node",
    "target": "ES6",
    "outDir": "dist",
    "lib": ["DOM", "ESNext"],
    "sourceMap": true,
    "rootDir": ".",
    "strict": true,
    "noImplicitAny": false,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@w/*": ["webview/*"],
      "luogu-api": ["luogu-api-docs/luogu-api"]
    },
    "esModuleInterop": true,
    "jsx": "react"
  },
  "include": ["./src", "./webview"]
}
